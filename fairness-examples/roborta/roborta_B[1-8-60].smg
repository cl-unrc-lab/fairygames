// the robot can move sideways freely in any other cell
// Roborta model
module Roborta
       col : [0..WIDTH-1] init 0;
       row : [0..LENGTH-1] init 0;
       [move_left] ((light=1) | (light=3)) & !MOVES_R & (row < LENGTH) -> (1-T) : (light'=0) & (col'=mod((col-1),WIDTH)) + (T) : (light'=0);
       [move_right] ((light=1) | (light=3)) & !MOVES_L & (row < LENGTH)  -> (1-T) : (light'=0) & (col'=mod((col+1), WIDTH)) + (T) : (light'=0) ;
       [move_forward] ((light=2) | (light=3)) & (row < LENGTH) -> (1-T) : (light'=0) & (row'=row+1) + (T) : (light'=0);
endmodule
// Light model
module Light
       [signal_green] (light=0) & (row < LENGTH) -> (1-Q) : (light'=2) + Q : (light'=3);
       [signal_yellow] (light=0) & (row < LENGTH) -> (light'=1);
endmodule
